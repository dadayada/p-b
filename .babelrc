{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "loose": true,
                "corejs": {
                    "version": 3,
                    "proposals": true
                }
            }
        ],
        "@babel/react",
        "@babel/preset-typescript",
        "patronum/babel-preset"
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        ["effector/babel-plugin", { "addLoc": true }]
    ]
}
